{
  "version": 2,
  "name": "jolvera.dev",
  "alias": ["jolvera.dev", "jolvera.now.sh"],
  "builds": [{ "src": "package.json", "use": "@now/next" }],
  "routes": [
    { "src": "/feed.json", "dest": "/_next/static/feed.json" },
    { "src": "/blog/(?<page>[^/]+)$", "dest": "/blog?page=$page" }
  ]
}
